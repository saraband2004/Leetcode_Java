import java.io.*; 
import java.util.*; 
import java.lang.*;


public class Solution {

	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Random RNGesus = new Random();
		int [] count = new int[10];
		for (int i=0; i<1000; i ++) {
		
		count [RNGesus.nextInt(10)]++;
		
		}
		for (int n: count) {
			System.out.print(n+" ");
		}
	}   
 

}
