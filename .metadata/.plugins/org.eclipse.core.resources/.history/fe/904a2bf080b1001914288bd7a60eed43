import java.io.*; 
import java.util.*; 
import java.lang.*;


public class Solution {
	enum Color 
	{ 
	     RED, GREEN, BLUE; 
	} 

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//System.out.print("aaa");
		String s="aaaaaaaaaa";
		StringBuilder str=new StringBuilder(s);
		str.append("abdc"); 
		str.setCharAt(5, 'x');
		System.out.println(str);
		
		Color c1 = Color.RED;
		System.out.println(c1); 
	}

}
