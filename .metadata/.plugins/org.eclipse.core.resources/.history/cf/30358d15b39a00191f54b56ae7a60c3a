class mod{
	public int value;
}


public class Solution {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	//		System.out.print("xxx");
	
			mod X = new mod();
			X.value=1;
			
			System.out.println(X.value);
			
			mod Y=X;
			System.out.println(Y.value);
			
	
	
	
	
	
	
	
	
	
	
	
	
	
	}
}
